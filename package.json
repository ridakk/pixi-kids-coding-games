{
  "name": "pixi-kids-coding-games",
  "version": "0.0.0",
  "description": "2D coding games for kids with PIXI.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "release": "standard-version",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ridakk/pixi-kids-coding-games.git"
  },
  "keywords": [
    "pixi",
    "2d",
    "coding",
    "games",
    "kids"
  ],
  "author": "Kadir Goktas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ridakk/pixi-kids-coding-games/issues"
  },
  "homepage": "https://github.com/ridakk/pixi-kids-coding-games#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.0.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-bundle": "^1.0.2",
    "eslint-plugin-dependencies": "^2.4.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-security": "^1.4.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.4.0",
    "standard-version": "^6.0.1",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^1.4.1",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@pixi/filter-glow": "^3.0.3",
    "@pixi/filter-outline": "^3.0.3",
    "es6-tween": "^5.5.10",
    "eventemitter3": "^4.0.0",
    "lodash": "^4.17.15",
    "normalize.css": "^8.0.1",
    "pixi-filters": "^3.0.3",
    "pixi-sound": "^3.0.3",
    "pixi.js": "^5.1.1",
    "webfontloader": "^1.6.28"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
